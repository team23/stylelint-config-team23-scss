{
  "name": "stylelint-config-team23-scss",
  "version": "1.0.0",
  "description": "Standard shareable config for stylelint",
  "keywords": [
    "stylelint",
    "stylelint-config",
    "scss",
    "team23"
  ],
  "author": "TEAM23",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/team23/stylelint-config-team23-scss"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "dependencies": {
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-config-team23-standard": "github:team23/stylelint-config-team23-standard#v1.0.0-rc9"
  },
  "devDependencies": {
    "@stylelint/prettier-config": "^2.0.0",
    "@stylelint/remark-preset": "^3.0.0",
    "eslint": "^8.3.0",
    "eslint-config-stylelint": "^15.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-regexp": "^1.6.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "np": "^7.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.0",
    "pretty-quick": "^3.1.3",
    "remark-cli": "^10.0.1",
    "stylelint": "^14.6.0"
  },
  "peerDependencies": {
    "stylelint": "^14.6.0"
  },
  "scripts": {
    "format": "prettier . --write",
    "lint:formatting": "prettier . --check",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:md": "remark . --quiet --frail --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*",
    "test": "jest",
    "watch": "jest --watch",
    "prepare": "husky install",
    "pretty-quick": "pretty-quick"
  },
  "prettier": "@stylelint/prettier-config",
  "eslintConfig": {
    "extends": [
      "stylelint"
    ],
    "globals": {
      "module": true,
      "require": true
    }
  },
  "remarkConfig": {
    "plugins": [
      "@stylelint/remark-preset"
    ]
  }
}
